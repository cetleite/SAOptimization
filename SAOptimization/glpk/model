set A;

#Parametros
param n1 {i in A};
param n2 {i in A};
param dist {i in A, j in A};

#Variavel
var x {i in A, j in A} integer, >= 0;	#numero de avioes i vendidos
var b {i in A, j in A} binary;			#se nodo i esta tem ligacao com nodo j

#Funcao objetivo
minimize custoTotal:
	sum{i in A} (x[i] * lucro[i]) - sum{i in A} (b[i] * preco[i]);

#Restricoes
s.t. limite_capac:
	sum{i in A} (x[i] * capac[i]) <= 100;

s.t. limite_deman{i in A}:
	x[i] <= deman[i] * b[i];

